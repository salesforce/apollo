                <activeByDefault>false</activeByDefault>
            </activation>
            <modules>
                <module>isolates</module>
                <module>isolate-ftesting</module>
            </modules>
        </profile>
        <profile>
            <id>mac-domain</id>
            <activation>
                <os>
                    <family>mac</family>
                </os>
            </activation>
            <modules>
                <module>domain-kqueue</module>
            </modules>
        </profile>
        <profile>
            <id>linux-domain</id>
            <activation>
                <os>
                    <name>linux</name>
                </os>
            </activation>
            <modules>
